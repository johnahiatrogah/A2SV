class Solution(object):
    def convertTemperature(self, celsius):
        """
        :type celsius: float
        :rtype: List[float]
        """
        
        # Kelvin = Celcius + 273.15
        # Farenheit = Celcius * 1.80 + 32.00
        return [celsius + 273.15, celsius * 1.80 + 32.00]
        

        
